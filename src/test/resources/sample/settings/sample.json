{
  "gwen": {
    "behavior": {
      "rules": "strict"
    },
    "report": {
      "overwrite": false
    },
    "web": {
      "sendKeys": {
        "clearFirst": false,
        "clickFirst": false
      },
      "wait": {
        "seconds": 9
      },
      "chrome": {
        "args": [
          "--ignore-certificate-errors",
          "--window-size=1920,1080"
        ]
      }
    }
  }
}